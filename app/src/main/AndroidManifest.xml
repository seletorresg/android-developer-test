<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hn.com.parking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.mainMenu.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.entry.EntryActivity">
        </activity>
        <activity android:name=".ui.exit.ExitActivity">
        </activity>
        <activity android:name=".ui.vehicle.VehicleActivity">
        </activity>
        <activity android:name=".ui.type.TypeActvity">
        </activity>
    </application>

</manifest>